<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style>
		<Style.Resources>
			<LinearGradientBrush x:Key="MainColor">
				<GradientStop Offset="0" Color="#08142E"/>
				<GradientStop Offset="0.5" Color="#0D1117"/>
			</LinearGradientBrush>
		</Style.Resources>

		<Style.Resources>
			<LinearGradientBrush x:Key="SecondaryColor">
				<GradientStop Offset="0" Color="#33298C "/>
				<GradientStop Offset="0.9" Color="#332E6B "/>
			</LinearGradientBrush>
		</Style.Resources>

		<Style.Resources>
			<LinearGradientBrush x:Key="AccentColor">
				<GradientStop Offset="0" Color="#2F03E6"/>
				<GradientStop Offset="1" Color="#FF01C1"/>
			</LinearGradientBrush>
		</Style.Resources>

		<Style.Resources>
			<LinearGradientBrush x:Key="TextColor">
				<GradientStop Offset="0.5" Color="#F904D2"/>
				<GradientStop Offset="1" Color="#C708F9"/>
			</LinearGradientBrush>
		</Style.Resources>
	</Style>
</Styles>
